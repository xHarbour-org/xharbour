           ;

/* Array expressions access
 */
VariableAt  : NilValue      ArrayIndex    { $$ = $2; }
            | CodeBlock     ArrayIndex    { $$ = $2; }
            | Logical       ArrayIndex    { $$ = $2; }
            | Date          ArrayIndex    { $$ = $2; }
            | DateTime      ArrayIndex    { $$ = $2; }
