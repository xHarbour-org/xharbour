/*
  ACE620.PRG
  This is a utility to create WatCom Import Library from ace32.dll as
  a ready-for-use ace32.lib for Watcom is not currently available.
  Please read comments on each sub-functions in this program file.

  How To Create ACE32.LIB for Open-Watcom 1.x:
  1. Compile this program
  2. Run ACE620.EXE in any directory ( file: ace32.def should be created )
  3. Run: LIB386 /out:ace32.lib /def:ace32.def
  4. Copy ace32.lib to your library folder
  5. Enjoy RDDADS with Watcom ;-)

  Andi Jahja

  2004-04-08 - First Release
*/
#define CRLF chr(13)+chr(10)
#xtranslate FWriteDEF( <x> ) => FWrite( h, <x> + CRLF )
#include "box.ch"
#include "inkey.ch"
//----------------------------------------------------------------------------//
PROCEDURE MAIN()

   LOCAL aWatCom := DLLWatcom()
   LOCAL aACE    := DLLAce_62()
   LOCAL cInScr  := SaveScreen(0,0,MaxRow(),MaxCol())
   LOCAL n, oldColor := SetColor()
   LOCAL cFileName := "ace32.def"
   LOCAL h       := FCreate( cFileName )
   LOCAL aDef    := {}
   LOCAL cDef, __cDef, nAt, __aDef, cEXE
   LOCAL cCommand

   IF( h < 0 )
      ALERT( "Error in creating file ...")
   ELSE
      OutStd( "Start job ..." + CRLF )
      FOR EACH cDef IN aWatCom
         __cDef := SubStr( cDef, 2 )
         nAt    := AT( "@", __cDef )
         __cDef := LEFT( __cDef, nAt - 1 )
         IF ( n := AScan( aACE, { |e| e[1] == __cDef }) ) > 0
            OutStd( "Creating definition for : " + __cDef + CRLF )
            AAdd( aDef, { space(5) + PADR( cDef,50 ) + aAce[n][2], strzero(val(strtran(aAce[n][2],"@")),4) } )
         ENDIF
      NEXT

      OutStd( "Finished ..." + CRLF )
      OutStd( "Sorting ..." + CRLF )
      ASort( aDef,,, { |a,b| a[2] < b[2] } )

      OutStd( "Writing file ..." + CRLF )
      FWriteDEF( "LIBRARY     ACE32.DLL" )
      FWriteDEF( "" )
      FWriteDEF( "EXPORTS" )

      FOR EACH __aDef IN aDef
         FWriteDEF( __aDef[1] )
      NEXT

      FClose( h )

      OutStd( "End of job ..." + CRLF )

      IF FILE( cFileName )
         IF ALERT( "ACE32.DEF created ("+ str(len(aDef),,,.T.) + " functions imported);Do you want to view the file ?", { " Yes ", " No "}, "W+/B" ) == 1
            SetColor( "W+/B" )
            View( cFileName )
            SetColor( oldColor )
         ENDIF
         cEXE := GetLibEXE()
         IF Empty( cEXE )
            ALERT( "To create ACE32.LIB please run:;LIB386 /out:ace32.lib /def:ace32.def;and place ACE32.LIB to library folder",{" Quit Now "},"W+/G" )
         ELSE
            IF ALERT( "Do you want to create ACE32.LIB now ?",{" Yes ", " No "},"N/BG*" ) == 1
               cCommand := cEXE + " /out:ace32.lib /def:ace32.def /machine:IX86"
               CLEAR SCREEN
               OutStd( cCommand + CRLF + CRLF )
               IF FILE( "ace32.lib" )
                  FErase( "ace32.lib" )
               ENDIF
               CreateLib( cCommand )
               IF FILE( "ace32.lib" )
                  Alert( "ACE32.LIB created!;Put the file into library folder", { " Quit Now " }, "W+/G" )
               ELSE
                  Alert( "Error in creating ACE32.LIB" )
               ENDIF
            ENDIF
         ENDIF
      ELSE
         Alert( "File creation error !" )
      ENDIF
   ENDIF

   RestScreen(0,0,MaxRow(),MaxCol(), cInScr )

   RETURN

