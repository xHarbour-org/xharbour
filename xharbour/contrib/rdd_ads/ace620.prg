STATIC PROCEDURE View( cFile )

   LOCAL o := HBEditor():New("",1,1,maxrow()-1,maxcol()-1,.F.,maxcol()-2)
   LOCAL nkey

   dispbox(0,0,maxrow(),maxcol(),B_SINGLE + ' ')
   dispoutat( 0,1, "[ " + cFile + " ]" )

   o:lWordWrap := .F.
   o:Loadfile( cFile )

   WHILE ( ( nKey := inkey() ) != K_ESC )
      o:MoveCursor(nKey)
   ENDDO

   RETURN

//----------------------------------------------------------------------------//
STATIC FUNCTION GetLibEXE()

   LOCAL cPath := getenv( "PATH" )
   LOCAL aPath := FParseLine( cPath, ";" )
   LOCAL i, lFound := .F., cEXE := ""

   FOR i := 1 TO LEN( aPath )
      IF FILE( cEXE := aPath[i] + "\LIB386.EXE" )
         lFound := .T.
         EXIT
      ENDIF
   NEXT

   RETURN IF( lFound, cEXE, "" )

