/*
   Program for:
    - testing availability of functions in a given version of ace32.dll
    - testing availability of interface functions in ace32.c
    - function lists are always based on the latest version of ace32.dll
      as documented in the corresponring ace.h. This test program uses
      version 10.0.0.3
    - the purpose of this program is to give info to programmers to not use
      functions with WARNING attribute as a consequence of the ace32.dll she
      is using.
    - WARNING is emmited when either function is not found in ace32.dll OR
      there is no documentation (prototype) in ace.h
    - compile and direct output to a text file ie.: apitest >api.txt
   By: Andi Jahja
*/

PROCEDURE MAIN()

   LOCAL aFunc:= ACE32DLL_FuncList()
   LOCAL i, j := len( aFunc ), lInit := .F.
   LOCAL aAce32_C := ACE32LIB_FuncList(), w := Len( aAce32_C )

   OutStd( "=============================================" +  hb_osnewline() )
   OutStd( "Availability of interface function in ace32.c" + hb_osnewline() )
   OutStd( "=============================================" +  hb_osnewline() )
   OutStd( hb_osnewline() )

   FOR i := 1 to j
      IF aScan( aAce32_C, { |e| e == aFunc[i] } ) > 0
         OutStd( "Interface Function: " + aFunc[i] + " OK.." + hb_osnewline() )
      ELSE
         OutStd( ">>> WARNING: Interface Function: " + aFunc[i] + " NOT FOUND..<<<" + hb_osnewline() )
      ENDIF
   NEXT

   OutStd( hb_osnewline() )

   OutStd( "==============================" +  hb_osnewline() )
   OutStd( "Using ace32.dll version " + ADSVERSION() + hb_osnewline() )
   OutStd( "==============================" +  hb_osnewline() + hb_osnewline() )

   FOR i := 1 TO j
      IF TESTACEAPI( aFunc[i] )
         OutStd( aFunc[i] + " OK .." + hb_osnewline() )
      ELSE
         OutStd( ">>>  WARNING: " + aFunc[i] + " NOT FOUND .. <<<" + hb_osnewline() )
      ENDIF
   NEXT


   RETURN

